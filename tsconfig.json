{
  "compilerOptions": {
    "declaration": true,
    "lib": [
      "es2017"
    ],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "module": "commonjs",
    "target": "ESNext",
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "resolveJsonModule": true,
    "rootDir": ".",
    "outDir": "./out",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@core/*": [
        "./src/core/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "e2e",
    "out",
    
  ],

  
}